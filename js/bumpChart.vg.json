{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "config": {"view": {"stroke": ""}},
    "title": "Military Powers ranked according to Global Firepower (2007 - 2022)",
    "width": "container",
    "height": 400,

    "data": {
        "url": "https://raw.githubusercontent.com/osir0002/3179/main/data/rankings2.csv"
      }, 

      "mark": {"type": "image", "width": 50, "height": 50, "line": true},
      "encoding": {

        "x": {"field": "Year", "type": "ordinal", "title": null, "axis": {"orient": "top"}},
        "y": {"field": "Rank", "type": "ordinal", "title": null},
        "order": {"field": "Rank", "type": "ordinal"},
        "url" : {"field": "img", "type": "nominal"},
        


        "tooltip": [
            {"field": "Country", "type": "nominal", "title": "Country"},
            {"field": "Rank", "type": "ordinal", "title": "Rank"},
            {"field": "Year", "type": "ordinal", "title": "Year"}
          ]
              
      }

  }